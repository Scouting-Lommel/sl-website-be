{
  "kind": "collectionType",
  "collectionName": "leaders",
  "info": {
    "name": "Leaders"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "occupation": {
      "type": "string"
    },
    "birthday": {
      "type": "date",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Man",
        "Vrouw",
        "Andere"
      ],
      "required": true
    },
    "totem": {
      "type": "string",
      "required": true
    },
    "memberNr": {
      "type": "string",
      "required": true
    },
    "active": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "image": {
      "model": "images"
    },
    "group_roles": {
      "collection": "group-roles"
    },
    "group": {
      "model": "groups"
    }
  }
}
