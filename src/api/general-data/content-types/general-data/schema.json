{
  "kind": "singleType",
  "collectionName": "general_datas",
  "info": {
    "singularName": "general-data",
    "pluralName": "general-datas",
    "displayName": "GeneralData",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "SiteName": {
      "type": "string"
    },
    "Url": {
      "type": "string"
    },
    "Logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Address": {
      "type": "richtext"
    },
    "GroupNumber": {
      "type": "string"
    },
    "VatNumber": {
      "type": "string"
    },
    "BankAccountNumber": {
      "type": "string"
    },
    "ContactItems": {
      "displayName": "ContactItem",
      "type": "component",
      "repeatable": true,
      "component": "general.contact-item"
    },
    "YearTheme": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::year-theme.year-theme"
    },
    "MainNavigation": {
      "type": "component",
      "repeatable": true,
      "component": "general.navigation-item"
    },
    "FooterNavigation": {
      "type": "component",
      "repeatable": true,
      "component": "general.footer-nav"
    }
  }
}
